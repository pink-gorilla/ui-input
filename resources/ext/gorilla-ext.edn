{:name "ui-input"
 :clj-require []
 :cljs-namespace [pinkgorilla.input.goldly]
 :cljs-bindings {}
 :cljs-ns-bindings {}
 ; css and pinkie not yet via loader
 :css {:components  {:ionslider {true ["ion-rangeslider/css/ion.rangeSlider.min.css"]}
                     :tailsui {true ["tails-ui/dist/index.css"]}
                     :gridlayout {true ["react-grid-layout/css/styles.css"
                                        "react-resizable/css/styles.css"]}}
