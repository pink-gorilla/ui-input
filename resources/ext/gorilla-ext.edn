             :category :input
             :id :slider-ion
             :filename "snippets/input/slider_ion.edn"}
            {:type :goldly
             :category :input
             :id :select
             :filename "snippets/input/select.edn"}
            {:type :pinkie
             :category :input
             :id :progress
             :filename "snippets/input/progress.edn"}
