             :id :checkbox
             :filename "snippets/input/checkbox.edn"}
            {:type :pinkie
             :category :input
             :id :button
             :filename "snippets/input/button.edn"}
            {:type :goldly
             :category :input
             :id :slider
             :filename "snippets/input/slider.edn"}
